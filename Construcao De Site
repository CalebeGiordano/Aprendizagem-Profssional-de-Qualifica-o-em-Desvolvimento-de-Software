create database AutoLaLocadora;

create table Marcas(
     id int auto_increment primary key,
     nome varchar(59) not null unique
);

create table Modelos(
 id int auto_incremet primary key,
nome varchar(59) not null,
id_marca int,
foreign key (id_marca) reference Marcas(id)
);

create table veiculos(
 id int auto_increment primary key,
 placa varchar(10) not null unique,
 ano int not null,
 cor varchar(20)
 id_modelo int,
 status enum(`Disponivel `,`locado`,`Em Manutencao`) default `Disponivel`
 foreign key (id_modelo) references Modelos(id)
;
